#!/usr/bin/env node
# -*- js   -*-
#


function re_download {
  rm -f "$1"
  wget -O "$1" "$2"
}


re_download "public/underscore.js" "http://underscorejs.org/underscore-min.js"
re_download "public/promise.js"    "https://raw.github.com/stackp/promisejs/master/promise.min.js"

